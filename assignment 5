#include <stdio.h>

int main()
{
   int i,x;
   printf("Enter the number");
   scanf("%d",&x);
   int count;
   count=0;
   
   while (x>0){
       if(x%10==3) count++;
       x/=10;
   }
    printf("there are %d 3's in this number\n",count);
    
    return 0;
}

int main()
{
    int num;
    printf("enter the number");
    scanf("%d", &num);
    int a;
    while (num>0){
        a=num%10;
        printf("%d",a);
        num/=10;
    }

    return 0;
}
